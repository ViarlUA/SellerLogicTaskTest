{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "doctrine/dbal": "^3.0",
        "glushkovds/phpclickhouse-laravel": "^1.19",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^11.0",
        "laravel/sanctum": "4.0",
        "laravel/tinker": "^2.7"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.0",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "laravel/telescope": "^5.0",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "check_style": [
            "./tools/php-cs-fixer/vendor/bin/php-cs-fixer fix -v --dry-run --show-progress=dots"
        ],
        "fix_style": [
            "./tools/php-cs-fixer/vendor/bin/php-cs-fixer fix -v --show-progress=dots"
        ],
        "psalm": [
            "./tools/psalm/vendor/bin/psalm"
        ],
        "phpstan": [
            "./tools/phpstan/vendor/bin/phpstan analyse --ansi --memory-limit 512M"
        ],
        "rector_check": [
            "./tools/rector/vendor/bin/rector process --dry-run"
        ],
        "rector_fix": [
            "./tools/rector/vendor/bin/rector process"
        ],
        "code_check":[
            "composer check_style",
            "composer psalm",
            "composer phpstan"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
